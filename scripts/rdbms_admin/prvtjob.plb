  declare
    cnt number := 0;
  begin
    select count(*) into cnt
    from obj$ o, seq$ s
    where o.obj# = s.obj# and o.name = 'JOBSEQLSBY';

    if cnt = 0 then
      
      execute immediate 'create sequence jobseqlsby ' ||
        'start with 1000000000 increment by 1 ' ||
        'minvalue 1000000000 maxvalue 1999999999 ' ||
        'cache 20 noorder cycle';
    end if;
  end;
  /
